{
  "name": "next-goat",
  "description": "Next.js starter made for goats ðŸ",
  "private": true,
  "version": "0.1.0",
  "author": {
    "name": "Philip Johnston",
    "email": "tunderadev@gmail.com",
    "url": "https://github.com/tundera"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tundera/next-goat.git"
  },
  "engines": {
    "node": ">=12",
    "yarn": ">=1.15"
  },
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "NODE_OPTIONS='--inspect' next",
    "generate": "graphql-codegen",
    "build:nexus-typegen": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' --transpile-only src/lib/graphql/schema",
    "build:next": "next build",
    "build": "yarn build:nexus-typegen && yarn build:next",
    "db:introspect": "prisma introspect --schema='db/schema.prisma' --print",
    "db:push": "prisma db push --preview-feature --schema='db/schema.prisma'",
    "db:studio": "prisma studio",
    "db:migrate:dev": "prisma migrate dev --schema='db/schema.prisma' --preview-feature",
    "db:migrate:reset": "prisma migrate reset --schema='db/schema.prisma' --preview-feature",
    "db:migrate:deploy": "prisma migrate deploy --schema='db/schema.prisma' --preview-feature",
    "db:migrate:status": "prisma migrate status --schema='db/schema.prisma' --preview-feature",
    "start": "next start",
    "test": "jest",
    "test:ci": "jest --runInBand",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint './{src,pages}/**/*' --ext .js,.jsx,.tsx,.ts",
    "format": "eslint './{src,pages}/**/*' --ext .js,.jsx,.tsx,.ts --fix",
    "log:coaches": "ts-node --project tsconfig.scripts.json scripts/log-coaches.ts",
    "log:players": "ts-node --project tsconfig.scripts.json scripts/log-players.ts",
    "log:teams": "ts-node --project tsconfig.scripts.json scripts/log-teams.ts"
  },
  "dependencies": {
    "@chakra-ui/icons": "^1.0.2",
    "@chakra-ui/react": "^1.1.2",
    "@emotion/react": "^11.1.4",
    "@emotion/styled": "^11.0.0",
    "@prisma/client": "^2.14.0",
    "a11y-react-emoji": "^1.1.2",
    "apollo-server-micro": "^2.19.1",
    "deepmerge": "^4.2.2",
    "dotenv": "^8.2.0",
    "framer-motion": "^3.2.0",
    "graphql": "^15.4.0",
    "graphql-request": "^3.4.0",
    "graphql-tag": "^2.11.0",
    "gray-matter": "^4.0.2",
    "mdx-prism": "^0.3.1",
    "next": "^10.0.5",
    "next-mdx-remote": "^2.1.1",
    "nexus": "^1.0.0",
    "nexus-plugin-prisma": "^0.28.0",
    "react": "^0.0.0-experimental-27659559e",
    "react-dom": "^0.0.0-experimental-27659559e",
    "react-icons": "^4.1.0",
    "react-query": "^3.5.11",
    "rehype": "^11.0.0",
    "rehype-autolink-headings": "^5.0.1",
    "remark-code-titles": "^0.1.1",
    "remark-external-links": "^8.0.0",
    "remark-slug": "^6.0.0",
    "remark-toc": "^7.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@graphql-codegen/cli": "^1.20.0",
    "@graphql-codegen/typescript": "^1.20.0",
    "@graphql-codegen/typescript-operations": "^1.17.13",
    "@graphql-codegen/typescript-react-query": "^1.0.0",
    "@prisma/cli": "^2.14.0",
    "@testing-library/react": "^11.2.3",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.20",
    "@types/react": "^17.0.0",
    "@types/react-addons-test-utils": "^0.14.25",
    "@types/react-dom": "^17.0.0",
    "@types/react-test-renderer": "^17.0.0",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^4.0.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "jest": "^26.6.3",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.2.1",
    "react-addons-test-utils": "^15.6.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3"
  }
}
